{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "cleanDistDir": true,
        "buildCommand": "npm run build"
      }
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
